<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.abc.ita_v30">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LiveCapture" />
        <activity android:name=".ChoosefromGallary" />
        <activity android:name=".ChoosefromGallary2" />
        <activity
            android:name=".OcrGraphic"
            android:label="@string/title_activity_ocr_graphic"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ui.camera.CameraSource"
            android:label="@string/title_activity_camera_source"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ui.camera.CameraSourcePreview"
            android:label="@string/title_activity_camera_source_preview"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ui.camera.GraphicOverlay"
            android:label="@string/title_activity_graphic_overlay"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>